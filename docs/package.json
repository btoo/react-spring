{
  "name": "@react-spring/docs",
  "license": "MIT",
  "author": "Josh Ellis",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "remix vite:build",
    "dev": "concurrently \"yarn dev:remix\" \"yarn scripts:watch\"",
    "dev:remix": "vite dev",
    "lint": "TIMING=1 eslint \"app/**/*.ts*\" \"scripts/**/*.ts*\"",
    "start": "remix-serve ./build/server/index.js",
    "scripts:build": "node --loader esbuild-register/loader -r esbuild-register ./scripts/build",
    "scripts:watch": "node --loader esbuild-register/loader -r esbuild-register ./scripts/watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@docsearch/react": "3.6.0",
    "@mdx-js/react": "3.0.1",
    "@react-spring/rafz": "workspace:^",
    "@react-spring/web": "workspace:^",
    "@remix-run/node": "2.9.2",
    "@remix-run/react": "2.9.2",
    "@remix-run/serve": "2.9.2",
    "@remix-run/server-runtime": "2.9.2",
    "@vercel/analytics": "1.2.2",
    "@vercel/remix": "2.9.2-patch.2",
    "isbot": "4",
    "jotai": "^2.8.0",
    "phosphor-react": "^1.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@mdx-js/rollup": "3.0.1",
    "@remix-run/dev": "2.9.2",
    "@types/react": "18.2.62",
    "@types/react-dom": "18.2.19",
    "chokidar": "3.5.3",
    "concurrently": "8.2.2",
    "esbuild": "0.20.2",
    "esbuild-register": "3.5.0",
    "gray-matter": "4.0.3",
    "hast-util-to-html": "7.1.3",
    "parse-numeric-range": "1.3.0",
    "refractor": "4.8.1",
    "rehype-autolink-headings": "7.1.0",
    "rehype-parse": "9.0.0",
    "rehype-slug": "6.0.0",
    "remark-directive": "3.0.0",
    "remark-frontmatter": "5.0.0",
    "remark-mdx-frontmatter": "4.0.0",
    "typescript": "5.3.3",
    "unified": "11.0.4",
    "unist-util-visit": "5.0.0",
    "vite": "5.1.0",
    "vite-tsconfig-paths": "4.2.1"
  }
}
